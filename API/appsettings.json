{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "[AZURE TENANT NAME].onmicrosoft.com",
    "TenantId": "[AZURE TENANT ID GUID]",
    "ClientId": "[API AZURE APP CLIENT ID GUID]",
    "ClientSecret": "[API AZURE APP CLIENT SECRET]"
  },
  "Roles": {
    "Admin": "AuthStarter.Admin",
    "User": "AuthStarter.User",
